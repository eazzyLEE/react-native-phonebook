{
  "name": "react-native-phonebook",
  "version": "1.0.1",
  "description": "React Native library to launch phonebook/contacts app and return selected contact details",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-phonebook.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "build": "bob build",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --max-warnings 0",
    "release": "release-it",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn && yarn example pod-install",
    "pod-install": "cd example/ios && pod install"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "contacts",
    "phonebook",
    "native-module"
  ],
  "repository": "https://github.com/eazzylee/react-native-phonebook",
  "author": "eazylee <israelohue@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eazzylee/react-native-phonebook/issues"
  },
  "homepage": "https://github.com/eazzylee/react-native-phonebook#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.6",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-builder-bob": "^0.20.0",
    "release-it": "^15.5.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community"
    ],
    "rules": {}
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
